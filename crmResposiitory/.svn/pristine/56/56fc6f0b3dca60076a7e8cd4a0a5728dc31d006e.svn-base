package com.lxit.crm.entity;

import java.io.Serializable;

public class Forecast implements Serializable {

	/**
	 * 个人预测表
	 */
	private static final long serialVersionUID = 1L;
	
	private int id;//预测表主键
	
	private String title;//预测标题
	
	private String date;//预测时间
	
	private String type;//预测类型
	
	private String userName;//个人预测姓名
	
	private String deptName;//部门预测名称
	
	private int userCount;//员工数量
	
	private int clientCouont;//客户数量
	
	private String startDate;//生成时间
	
	private String handleUserName;//操作人名字
	
	private String text;//预测结果
	
	private String vdate;
	
	private double goalMoney;
	
	

	public double getGoalMoney() {
		return goalMoney;
	}

	public void setGoalMoney(double goalMoney) {
		this.goalMoney = goalMoney;
	}

	public String getVdate() {
		return vdate;
	}

	public void setVdate(String vdate) {
		this.vdate = vdate;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		vdate = date.substring(0,7);
		this.date = date;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getDeptName() {
		return deptName;
	}

	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}

	public int getUserCount() {
		return userCount;
	}

	public void setUserCount(int userCount) {
		this.userCount = userCount;
	}

	public int getClientCouont() {
		return clientCouont;
	}

	public void setClientCouont(int clientCouont) {
		this.clientCouont = clientCouont;
	}

	public String getStartDate() {
		return startDate;
	}

	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}

	public String getHandleUserName() {
		return handleUserName;
	}

	public void setHandleUserName(String handleUserName) {
		this.handleUserName = handleUserName;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	@Override
	public String toString() {
		return "Forecast [id=" + id + ", title=" + title + ", date=" + date + ", type=" + type + ", userName="
				+ userName + ", deptName=" + deptName + ", userCount=" + userCount + ", clientCouont=" + clientCouont
				+ ", startDate=" + startDate + ", handleUserName=" + handleUserName + ", text=" + text + "]";
	}
	
	
	
	

}
