package com.lxit.crm.entity;

public class ServiceUser {
	private Service serivce;
	private User user;

	public Service getSerivce() {
		return serivce;
	}

	public void setSerivce(Service serivce) {
		this.serivce = serivce;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public ServiceUser(Service serivce, User user) {
		super();
		this.serivce = serivce;
		this.user = user;
	}

	@Override
	public String toString() {
		return "ServiceUser [serivce=" + serivce + ", user=" + user + "]";
	}

}
