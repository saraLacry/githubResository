<body>
	服务管理>> 服务创建>> 创建服务
	<!-- 数据表格 -->
	<table id="roleManage">
		<div id="tbd">
			<!-- 进行选项查询 -->
			<!-- 添加一条服务我需要        添加一个名称 类型和状态需要使用字典 最后操作时间 跟添加这条数据的人
		     操作人就是最后一个操作这条数据的人  待处理人默认就是我的上级  
		     添加一个客户名  服务内容 客户电话 和信息 
		  -->
			<form action="" method="post">
				服务名称：<input type="text" id="issueName" value=""> 
				类型： <select name="issueType" id="issueType">
					<option value="业务咨询">业务咨询
					<option value="售后问题">售后问题
					<option value="投诉问题">投诉问题
					<option value="售前问题">售前问题
				</select>
				</select> <input type="button" id="muhu" value="查询">
			</form>

			<hr />


			<!-- 进行选项查询 -->
			<form action="" method="post">
				<center>
					&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;服务信息列表<input type="button" value="添加"
						id="commonInsert"> <input type="button" value="修改"
						id="commonUpdate"> <input type="button" value="删除"
						id="commonUpdatedelete">
						<input type="button" value="查看" id="commonqueryc">
						
				</center>
			</form>
		</div>
	</table>
	<!-- insert页面 -->
	<div id="CommorissuecommonInsert"></div>
	<!-- update页面 -->
	<div id="CommorissuecommonUpdate"></div>
	<!-- 查询 -->
	<div id="Commorissuecommonquery"></div>
	<!-- 自定义导入js -->
	<script type="text/javascript" src="js/CommonIssue.js"></script>
