<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>角色增加</title>
</head>
<body id="bodyids">
	<div style="margin-left: 400px;">
		服务管理>> 服务添加>> 查询服务 <br> <br> <br>
		<table>

			<input type="hidden" id="serviceId" name=serviceId
				value="${phqueryo.serviceId}" /> 服务名称：
			<input type="text" height="50px" disabled="disabled"
				disabled="disabled" value="${phqueryo.serviceName}" /> 服务类型：
			<input type="text" height="50px" id="beizhuIds" disabled="disabled"
				value="${phqueryo.serviceType}" />


			<tr>
				<td>服务客户：</td>
				<td><input type="text" height="50px"
					value="${phqueryo.clientName}" disabled="disabled" /></td>
				<td>联系电话：</td>
				<td><input type="text" height="50px" id="phoneClient"
					name="phoneClient" disabled="disabled"
					value="${phqueryo.phoneClient}" /></td>


			</tr>
			<tr>
				<td>创建人:</td>
				<td><input type="text" height="50px"
					value="${phqueryo.foundidName}" disabled="disabled" /></td>
			<td>服务状态:</td>
				<td><input type="text" height="50px"
					value="${phqueryo.serviceState}" disabled="disabled" /></td>
			</tr>
			<TR>
				<TD class=field>服务内容：</TD>
				<TD><TEXTAREA id="serviceContent" disabled="disabled"
						name="serviceContent">${phqueryo.serviceContent}</TEXTAREA></TD>
			</TR>
			<TR>
				<TD class=field>服务详细：</TD>
				<TD><TEXTAREA id="remark" disabled="disabled" name="remark">${phqueryo.remark }</TEXTAREA></TD>
			</TR>

			<TR>
				<TD class=field>处理详细：</TD>
				<TD><TEXTAREA id="remark" disabled="disabled" name="remark">${phqueryo.disposeExplain}</TEXTAREA></TD>
			</TR>

			<TR>
				<TD class=field>反馈详细：</TD>
				<TD><TEXTAREA id="remark" disabled="disabled" name="remark">${phqueryo.feedbackExplain}</TEXTAREA></TD>
			</TR>
		</table>


	</div>
	<center></center>
	<script type="text/javascript">
		
	</script>
</body>
</html>